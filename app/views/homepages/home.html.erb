<h1>Welcome to Images Blog</h1>

<ul class="list-group">
  <li class="list-group-item"><%= link_to 'Albums', albums_path %></li>
  <% if logged_in?%>
    <li class="list-group-item"><%= link_to 'logout', logout_path, method: :delete %></li>
<% else %>
    <li class="list-group-item"><%= link_to 'Login', login_path() %></li>
    <li class="list-group-item"><%= link_to 'Signup', signup_path() %></li>
  <% end %>

</ul>
<%= render 'pictures/picture' %>